<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <PackageId>YperConnector</PackageId>
    <Authors>Alexandre Dubois</Authors>
    <Company>Objectif 33 SAS</Company>
    <Description>
      This client library enables working with Yper delivery service.
      Project repository and documentation - https://github.com/alexandredubois/yper-connector
      Yper API Documentation - https://yper.stoplight.io/docs/ypercore-documentation-public/docs/1%20-%20Before%20starting/0-overview.md
      Yper website - https://www.yper.fr/
    </Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.WebApi.Client" Version="5.2.7" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
